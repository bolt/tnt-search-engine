{
    "name": "bolt/tnt-search-engine",
    "description": "A search engine for Bolt using TNTSearch",
    "type": "bolt-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivo Valchev",
            "email": "ivo.v.valchev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "twig/twig": "^2.12 | ^3.0",
        "teamtnt/tntsearch": "^2.8"
    },
    "require-dev": {
        "bolt/core": "^4.1 || ^5.0",
        "symplify/easy-coding-standard": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\TntSearch\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "entrypoint": "Bolt\\TntSearch\\Extension"
    }
}
